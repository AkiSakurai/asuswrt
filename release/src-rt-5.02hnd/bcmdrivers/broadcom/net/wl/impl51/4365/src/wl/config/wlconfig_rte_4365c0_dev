IGMP_UCQUERY=1
UCAST_UPNP=1
MCAST_REGEN=1
RXCHAIN_PWRSAVE=1
SMF_STATS=1
PSPRETEND=1
SCB_BS_DATA=1
WLINTFERSTAT=1

#MUMIMO support
BCM_DMA_INDIRECT=1
